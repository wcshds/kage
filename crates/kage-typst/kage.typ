#let p = plugin("./kage_typst.wasm")
#let kage-to-svg(data, name) = {
  let joined-data = data.pairs().map(each => each.at(0) + "|" + each.at(1).replace(regex(`\n+`.text), "$")).join("\n")

  p.kage_to_svg(bytes(joined-data), bytes(name))
}

